using System;


namespace Product
{
    class Program
    {
        static void Main(string[] args)
        {
            int ni, f1, f2, fib;
            Console.WriteLine("Enter no of Iterations that you want");
            ni = int.Parse(Console.ReadLine());
            f1 = 0;
            f2 = 1;
            Console.WriteLine("\n" + f1);
            Console.WriteLine("\n" + f2);
            for (int i = 1; i <= ni; i++)
            {
                fib = f1 + f2;
                f1 = f2;
                f2 = fib;
                Console.WriteLine("\n" + fib);
            }
            Console.ReadKey();
        }
    }

}
